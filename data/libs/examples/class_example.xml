<!-- Use skillplanner for the root tag. This is required -->
<skillplanner>

	<!-- put a short description of the class into the description tag -->
	<description> Description of the suclass goes heere </description>
	
	<!-- Put the name of the class here. This is the name that will show in the skill planner -->
	<name> The name of the class </name>
	
	<!-- use the skill tag to indicate a skill. 
	Use the file name for the skill. If you want a list of available skills
	then run the skilldump.jar. All general skills are loaded automatically -->
	<skill>TripleSlash</skill>
	<skill>GhostSlash</skill>
	

</skillplanner>